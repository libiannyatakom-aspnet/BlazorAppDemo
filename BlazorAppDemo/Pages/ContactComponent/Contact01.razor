<p>
    Name : <b>@first_name @last_name</b><br>
    Email : <span>@email</span><br>
    Gender : <span>@gender</span>
</p>

@code {
    [Parameter]
    public string first_name { get; set; }

    [Parameter]
    public string last_name { get; set;  }

    [Parameter]
    public string email { get; set; }

    [Parameter]
    public string gender { get; set; }
}
